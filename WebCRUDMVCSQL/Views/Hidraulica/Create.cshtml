@model ObraFacilApp.Models.HidraulicaModel

@{
    ViewData["Title"] = "Create";
}


<h4>Hidraulica</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <input type="hidden" asp-for="ProjetoId" value="@ViewBag.ProjetoId" />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="QtdTorneiras" class="control-label"></label>
                <input asp-for="QtdTorneiras" class="form-control" />
                <span asp-validation-for="QtdTorneiras" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QtdRalos" class="control-label"></label>
                <input asp-for="QtdRalos" class="form-control" />
                <span asp-validation-for="QtdRalos" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QtdRegistros" class="control-label"></label>
                <input asp-for="QtdRegistros" class="form-control" />
                <span asp-validation-for="QtdRegistros" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QtdCaixaGordura" class="control-label"></label>
                <input asp-for="QtdCaixaGordura" class="form-control" />
                <span asp-validation-for="QtdCaixaGordura" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PrevisaoCusto" class="control-label"></label>
                <input asp-for="PrevisaoCusto" class="form-control" />
                <span asp-validation-for="PrevisaoCusto" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataInicioHidraulica" class="control-label"></label>
                <input asp-for="DataInicioHidraulica" type="date" class="form-control" />
                <span asp-validation-for="DataInicioHidraulica" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataConclusaoHidraulica" class="control-label"></label>
                <input asp-for="DataConclusaoHidraulica" type="date"  class="form-control" />
                <span asp-validation-for="DataConclusaoHidraulica " class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Criar" class="btn btn-primary" style="background-color:#255f9c" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

