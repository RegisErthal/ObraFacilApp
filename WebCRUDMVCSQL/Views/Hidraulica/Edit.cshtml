@model ObraFacilApp.Models.HidraulicaModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Hidraulica</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="ProjetoId" class="control-label"></label>
                <input asp-for="ProjetoId" class="form-control" />
                <span asp-validation-for="ProjetoId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QtdTorneiras" class="control-label"></label>
                <input asp-for="QtdTorneiras" class="form-control" />
                <span asp-validation-for="QtdTorneiras" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QtdRalos" class="control-label"></label>
                <input asp-for="QtdRalos" class="form-control" />
                <span asp-validation-for="QtdRalos" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataInicioHidraulica" class="control-label"></label>
                <input asp-for="DataInicioHidraulica" class="form-control" />
                <span asp-validation-for="DataInicioHidraulica" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DataConclusaoHidraulica" class="control-label"></label>
                <input asp-for="DataConclusaoHidraulica" class="form-control" />
                <span asp-validation-for="DataConclusaoHidraulica" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" style="background-color:#255f9c" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

