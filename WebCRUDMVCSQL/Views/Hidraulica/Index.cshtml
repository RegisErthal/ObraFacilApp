@model IEnumerable<ObraFacilApp.Models.Hidraulica>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdProjeto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QtdTorneiras)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QtdRalos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataInicioEletrica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataConclusaoEletrica)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdProjeto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtdTorneiras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QtdRalos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataInicioEletrica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataConclusaoEletrica)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
<p>
    <a class="btn btn-primary" asp-action="Create">Criar</a>
</p>